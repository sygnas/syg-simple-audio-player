{
    "extends": [
        "airbnb"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": ["error", "windows"],
        "camelcase": ["off"],
        "prefer-destructuring": ["off"],
        "no-use-before-define": ["error", { "functions": false }]
    }
}
